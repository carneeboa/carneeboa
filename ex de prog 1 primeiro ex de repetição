#include <stdio.h>
int main() 
{ 
  int n1, n2;
  printf("Digite o primeiro numero que voce quer o entre eles: ");
  scanf("%i",&n1);
  
 printf("Digite o segundo numero que voce quer o entre eles: ");
  scanf("%i",&n2);
    ++n1;
  
    
  while (n1 < n2) {
    printf("%d\n", n1);
    ++n1;
  }

  return 0;
}
