#include <stdio.h>
int main()
{
  int n1, n2, n3, n4, n5, n6, n7;
  printf("Digite n1: ");
  scanf("%i",&n1);
  
  printf("Digite n2: ");
  scanf("%i",&n2);
  
  n3 = n1 % n2 ;
  switch (n3){
  case 1:
    n4 = n3 + n2 + n1;
    printf ("%i", n4);
    break;
  case 2:
    printf ("2");
    break;
  case 3:
    n4 = n1 + n2;
    n5 = n4 * n1;
      printf ("%i", n5);
      break;
  case 4: 
    n4 = (n1 + n2) /n1;
      printf ("%i", n4);
      break;}
  if (n3 > 4 || n3 == 0){
      printf ("5");
      }
  
    
      
  
  
  return 0;
}
  
  
